echo "DISTRO is: $DISTRO"
echo "REPO is: $REPO"
rm -f "/etc/apt/sources.list.d/xpra.sources"



REPO="xpra"
echo "REPO is: $REPO"


echo "deb [signed-by=/usr/share/keyrings/xpra.asc] https://xpra.org/ noble main" | tee /etc/apt/sources.list.d/xpra.list



apt update
apt install xpra



xpra --version


apt install -y tasksel


tasksel install xubuntu-desktop


apt update
apt install -y xfce4-session


apt update
apt install -y xubuntu-desktop xfce4 xfce4-goodies




