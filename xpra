apt-get update && apt-get upgrade && apt update && apt upgrade && apt install aptitude && aptitude full-upgrade

sudo apt install gnupg

DISTRO="noble"

echo "DISTRO is: $DISTRO"
echo "REPO is: $REPO"
rm -f "/etc/apt/sources.list.d/xpra.sources"



REPO="xpra"
echo "REPO is: $REPO"




ls -l /usr/share/keyrings/xpra.asc
head -5 /usr/share/keyrings/xpra.asc



wget -O "/usr/share/keyrings/xpra.asc" https://xpra.org/xpra.asc


gpg --dearmor -o /usr/share/keyrings/xpra.gpg /usr/share/keyrings/xpra.asc


gpg --show-keys /usr/share/keyrings/xpra.gpg


cat /etc/apt/sources.list.d/xpra.sources


echo "Types: deb
URIs: https://xpra.org/
Suites: noble
Components: main
Signed-By: /usr/share/keyrings/xpra.gpg" | tee /etc/apt/sources.list.d/xpra.sources


chmod 644 /etc/apt/sources.list.d/xpra.sources
chmod 644 /usr/share/keyrings/xpra.gpg








echo "deb [signed-by=/usr/share/keyrings/xpra.asc] https://xpra.org/ noble main" | tee /etc/apt/sources.list.d/xpra.list



apt update
apt install xpra



xpra --version


apt install -y tasksel


tasksel install xubuntu-desktop


apt update
apt install -y xfce4-session


apt update
apt install -y xubuntu-desktop xfce4 xfce4-goodies



xpra start :100 --start-child=xfce4-session --exit-with-children --bind-tcp=0.0.0.0:8080 --html=on



netstat -tuln | grep 8080


cat /run/xpra/100/server.log


xpra attach tcp:34.68.68.253:8080


killall xpra
rm -rf /run/xpra/100


http://34.45.185.44:8080


apt install 'dbus*'
